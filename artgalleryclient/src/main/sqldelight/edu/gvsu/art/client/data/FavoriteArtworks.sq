CREATE TABLE  IF NOT EXISTS FavoriteArtworks (
    id TEXT NOT NULL PRIMARY KEY,
    created_at TEXT DEFAULT CURRENT_TIMESTAMP NOT NULL
);

insert:
INSERT INTO FavoriteArtworks(id)
VALUES (  ?);

delete:
DELETE FROM FavoriteArtworks
WHERE id = ?;

favoriteExists:
SELECT EXISTS (
  SELECT id
  FROM FavoriteArtworks
  WHERE id = ?
  LIMIT 1
);

findAll:
SELECT
  Artworks.*
FROM FavoriteArtworks
JOIN Artworks ON FavoriteArtworks.id = Artworks.id
ORDER BY FavoriteArtworks.created_at DESC;
